---
title: Techies Lab
---

<%= partial "layouts/langbar" %>
<!-- background image -->
<div class="image-bg">
  <div class="bg-filter"></div>
</div>

<!-- Main -->
<section class="main text-center" id="home">
  <div class="page">
    <div class="wrapper">
      <div class="container">
        <%= image_tag "logo/robot.png", alt: "Logo", class: "main-logo" %>
        <h1 class="heading"><%= t('.header.title') %><br><span id="name">Techies Lab</span></h1>
        <p id="tagline">
          <span><%= t('.header.subtitle') %></span>
        </p>
        <!-- <div class="row">
          <div id="countdown"></div>
        </div> -->
        <!-- Subscription form -->
        <form class="signup" action="//techieslab.us11.list-manage.com/subscribe/post-json?u=4e5edbd56b4da2771ce53024c&amp;id=1c0ef6f860&c=?" role="form" id="signupForm">
          <div class="input-group" style="margin: 0 auto;">
            <div class="form-group pull-left">
              <input type="email" name="EMAIL" class="form-control" placeholder="<%= t('.header.email') %>">
            </div>
            <button type="submit" class="btn btn-theme ladda-button" data-style="expand-left"><span class="ladda-label"><%= t('.header.button.notification') %></span></button>
          </div>
          <div id="mce-responses" class="clear">
            <div id="mce-error-response" class="response alert alert-danger alert-dismissible" style="display:none" role="alert">
              <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times;</span></button>
              <span class="answer"></span>
            </div>
            <div id="mce-success-response" class="response alert alert-success alert-dismissible" style="display:none" role="alert">
              <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times;</span></button>
              <span class="answer"></span>
            </div>
          </div>
          <!-- real people should not fill this in and expect good things - do not remove this or risk form bot signups-->
          <div style="position: absolute; left: -5000px;"><input type="text" name="b_4e5edbd56b4da2771ce53024c_1c0ef6f860" tabindex="-1" value="">
          </div>
          <div class="clear"></div>
        </form>
        <footer class="text-center">
          <div class="social">
            <a href="https://twitter.com/TechiesLab" class="btn btn-theme"><i class="fa fa-twitter"></i></a>
            <a href="https://www.facebook.com/TechiesLabBrussels" class="btn btn-theme"><i class="fa fa-facebook"></i></a>
            <!-- <a href="#" class="btn btn-theme"><i class="fa fa-google-plus"></i></a>
            <a href="#" class="btn btn-theme"><i class="fa fa-dribbble"></i></a>
            <a href="#" class="btn btn-theme"><i class="fa fa-linkedin"></i></a> -->
          </div>
        </footer>
      </div>
      <div class="arrow-down">
        <a class="scroll" href="#about" id="scroll"><i class="fa fa-angle-double-down"></i></a>
      </div>
    </div>
  </div>
</section>

<!-- About -->
<section id="about" class="section">
  <div class="container">
    <div class="row">
      <div class="col-sm-12">
        <h3 class="section-title"><%= t('.about.title') %></h3>
        <div class="section-title-border"></div>
      </div>
    </div>
    <div class="row">
      <div class="col-sm-12">
        <p class="lead">
          <%= t('about.lead.intro') %> <%= t('about.lead.drop') %> <%= t('about.lead.text') %>
        </p>
        <p class="lead"><%= t('about.intro') %>
        </p>


<!--         <div class="row">
          <% data.tracks.each do |track| %>
            <div class="col-sm-6 feature-box-container">
              <div class="feature-box wow fadeInDown">
                <div class="icon">
                  <i class="fa fa-<%= track.icon %>"></i>
                </div>
                <div class="description">
                  <h4><%= link_to t(track.name), track.file %></h4>
                  <p id="description"><%= t(track.description) %>
                  </p>
                  <p><%= link_to t('about.read_more'), track.file, class: "btn btn-theme" %></p>
                </div>
              </div>
            </div>
          <% end %>
        </div> -->
      </div>
    </div>
  </div>
</section>

<!-- Upcoming Workshops -->
<section id="workshops" class="section">
  <div class="container">
    <div class="row">
      <div class="col-sm-12">
        <h3 class="section-title"><%= t('.upcoming.title') %></h3>
        <div class="section-title-border"></div>
      </div>
    </div>
    <div class="row">
      <div class="col-sm-12">
        <div class="row">
          <% data.all_events.each do |event| %>
            <div class="col-sm-6 feature-box-container">
              <div class="feature-box wow fadeInDown">
                <div class="icon">
                  <i class="fa fa-<%= event.icon %>"></i>
                </div>
                <div class="description">
                  <h4><%= link_to t(event.title), event.file %></h4>
                  <p id="description"></p>
                  <p><strong><%= t(event.date_drop) %></strong>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<%= t(event.date_text) %></p>
                  <p><strong><%= t(event.time_drop) %></strong>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<%= t(event.time_text) %></p>
                  <p><strong><%= t(event.location_drop) %></strong>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<%= t(event.short_location_text) %></p>
                  <p><strong><%= t(event.age_drop) %></strong>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<%= t(event.age_text) %></p>
                  <p><%= link_to t(event.button), event.file, class: "btn btn-theme", target: "_blank" %></p>
                </div>
              </div>
            </div>
          <% end %>
        </div>
      </div>
    </div>
  </div>
</section>


<!-- Team -->
<section id="team" class="section">
  <div class="container">
    <div class="row">
      <div class="col-sm-12">
        <h3 class="section-title"><%= t('.team.title') %></h3>
        <div class="section-title-border"></div>
      </div>
    </div>
    <div class="row text-center">
      <% data.team_members.each do |member| %>
        <div class="col-sm-3 text-center team-member-container wow fadeInDown">
          <div class="team-member">
            <div class="team-member-img">
              <%= image_tag member.image, alt: member.name, class: "img-responsive" %>
              <div class="team-member-img-filter"></div>
            </div>
            <div class="team-member-meta">
              <h4><%= member.name %></h4>
              <span class="team-member-profession"></span>
              <p><%= t(member.description) %></p>
              <div class="social">
                <% member.socials.each do |social| %>
                  <% link_to social.url, class: "btn btn-theme" do %>
                    <i class="fa fa-<%= social.icon %>"></i>
                  <% end %>
                <% end %>
              </div>
            </div>
          </div>
        </div>
      <% end %>
    </div>
  </div>
</section>

<!-- Contact -->
<section id="contact" class="section">
  <div class="container">
    <div class="row">
      <div class="col-sm-12">
        <h3 class="section-title"><%= t('.contact.title') %></h3>
        <div class="section-title-border"></div>
      </div>
    </div>
    <div class="row">
      <div class="col-sm-6">
        <p>
          <strong><%= t('.contact.intro.drop') %></strong> <%= t('.contact.intro.text') %>
        </p>
        <ul class="list-contacts">
          <!-- <li><i class="fa fa-phone"></i> <%= data.settings.contact.tel %></li>
          <li><i class="fa fa-mobile-phone"></i> <%= data.settings.contact.cellphone %></li> -->
          <li><i class="fa fa-map-marker"></i> <a href="https://www.google.be/maps/place/Co.Station/@50.8473558,4.3589762,15z/data=!4m2!3m1!1s0x0:0xa083255f82851545?sa=X&ved=0CHUQ_BIwC2oVChMIxp_Wp6DmyAIVig8aCh34eQ8K"><%= data.settings.contact.street %><br> <%= data.settings.contact.city %></a></li>
          <li><i class="fa fa-envelope-o"></i> <a href="mailto:<%= data.settings.contact.email %>"><%= data.settings.contact.email %></a></li>
          <li><i class="fa fa-twitter"></i> <a href="https://twitter.com/TechiesLab"><%= data.settings.social.twitter %></a></li>
          <li><i class="fa fa-facebook"></i> <a href="https://www.facebook.com/TechiesLabBrussels"><%= data.settings.social.facebook %></a></li>
        </ul>
      </div>
      <div class="col-sm-6">
        <form class="form-horizontal" action="//formspree.io/anne@techieslab.org" method="post" role="form" id="contactForm">
          <div class="form-group">
            <div class="col-sm-6">
              <input type="text" name="name" class="form-control" placeholder="<%= t('.contact.form.name') %>">
            </div>
            <div class="col-sm-6">
              <input type="text" name="contactEmail" class="form-control" placeholder="<%= t('.contact.form.email') %>">
            </div>
          </div>
          <div class="form-group">
            <div class="col-sm-12">
              <textarea name="message" class="form-control" rows="8" placeholder="<%= t('.contact.form.message') %>"></textarea>
            </div>
          </div>
          <div class="form-group">
            <div class="col-sm-12">
              <button type="submit" class="btn btn-theme ladda-button" data-style="expand-left">
                <span class="ladda-label"><%= t('.contact.form.submit') %></span>
              </button>
            </div>
          </div>
          <input type="text" name="_gotcha" style="display:none" />
        </form>
      </div>
    </div>
    <div class="row text-center copyright">
      <div class="col-sm-12">Â© TechiesLab.org 2015 - Designed and built in Belgium with <i class="fa fa-heart-o"></i></div>
    </div>
  </div>
</section>
